<header class="container">
  <h1>Welcome to Music Wall</h1>
</header>

<section class="container">

  <a class="btn btn-primary" href="/songs/new" role="button">Add new song</a>
  
  <% @songs.each do |song| %>
    <a href="<%= song.url %>" target="_blank">
      <div class="song">
        Title : <%= song.title %>
        by : <%= song.author %>
        <div>
          link : <%= song.url %>
        </div>
      </div>
    </a>
  <% end %>

</section>